dconf dump /org/nemo/ > nemo
dconf dump /org/cinnamon/desktop/applications/terminal/ > terminal
dconf dump /org/gtk/settings/file-chooser/ > gtk-file-chooser

dconf load /org/nemo/ < nemo
dconf load /org/cinnamon/desktop/applications/terminal/ < terminal
dconf load /org/gtk/settings/file-chooser/ < gtk-file-chooser

# blueman-manager will assume an action-capable notification daemon without
# this
dconf write /org/blueman/general/notification-daemon false
